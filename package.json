    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
