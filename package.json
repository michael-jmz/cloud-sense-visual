{
  "name": "cloud-sense-visual",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:href": "ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "delete:docs": "del docs",
    "copy:dist": "copyfiles dist/cloud-sense-visual/browser/* ./docs -f",
    "build:gitpages": "npm run delete:docs && npm run build:href && npm run copy:dist",
    "serve:ssr:cloud-sense-visual": "node dist/cloud-sense-visual/server/server.mjs"
  },
  "private": true,
  "dependencies": {
